<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyApplication" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
    </style>



    <style name="DefaultTheme" parent="Theme.Material3.DayNight">

        <item name="android:statusBarColor">@color/black</item>

        <item name="android:navigationBarColor">@color/black</item>

        <item name="android:windowBackground">@color/black</item>

        <item name="windowActionBar">false</item>

        <item name="windowNoTitle">true</item>

        <item name="android:windowFullscreen">true</item>

        <item name="colorApp">@color/black</item>


        <!-- Custom Color Attributes -->
        <item name="colorTextView">@color/white</item>
        <item name="colorError">#EF5350</item>
        <item name="colorSuccess">@color/blue</item>
        <item name="colorWarning">@color/magenta</item>
    </style>

</resources>